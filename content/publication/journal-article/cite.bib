@Article{18073433,
author={Zhang, Fengrong
and Pasalic, Enes
and Wei, Yongzhuang
and Cepak, Nastja},
title={Constructing bent functions outside the Maiorana-McFarland class using a general form of Rothaus},
year={2017},
volume={63},
number={8},
pages={5336-5349},
keywords={Rothaus konstrukcija; ukrivljene funkcije; razred Maiorana McFarland; nenormalne ukrivljene funkcije; {\v{s}}ibko normalne ukrivljene funkcije; Rothaus construction; bent functions; Maiorana-McFarland class; partial spread; non-normal bent functions; weakly normal bent functions},
abstract={In the mid 1960s, Rothaus proposed the so-called {\&}quot;most general form{\&}quot; of constructing new bent functions by using three (initial) bent functions whose sum is again bent. In this paper, we utilize a special case of Rothaus construction when two of these three bent functions differ by a suitably chosen characteristic function of an ▫{\$}n/2{\$}▫-dimensional subspace. This simplification allows us to treat the induced bent conditions more easily, also implying the possibility to specify the initial functions in the partial spread class and most notably to identify several instances of the so-called non-normal bent functions. Affine inequivalent bent functions within this class are then identified using a suitable selection of initial bent functions within the partial spread class (stemming from the complete Desarguesian spread). It is also shown that when the initial bent functions belong to the class ▫{\$}{\backslash}mathcal{\{}D{\}}{\$}▫, then, under certain conditions, the constructed functions provably do not belong to the completed Maiorana-McFarland class. We conjecture that our method potentially generates an infinite class of non-normal bent functions (all tested ten-variable functions are non-normal but unfortunately they are weakly normal) though there are no efficient computational tools for confirming this.},
note={Soavtorji: Enes Pasalic, Yongzhuang Wei, Nastja Cepak},
note={Bibliografija: str. 5348},
note={Abstract},
note={512.5},
issn={0018-9448},
url={http://ieeexplore.ieee.org/stamp/stamp.jsp?arnumber=7940075},
url={doi:10.1109/TIT.2017.2712598}
}

@Misc{17986905,
author={Zhang, Fengrong
and Pasalic, Enes
and Cepak, Nastja
and Wei, Yongzhuang},
title={Bent functions in C and D outside the completed Maiorana-McFarland class},
year={2017},
pages={298-313},
keywords={ukrivljene funkcije; razred C in D; Maiorana-McFalrand razred; bent functions; C and D class; Maiorana-McFarland class},
abstract={Two new classes of bent functions derived from the Maiorana-McFarland (▫{\$}{\backslash}mathcal{\{}M{\}}{\$}▫) class, so-called ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ and ▫{\$}{\backslash}mathcal{\{}D{\}}{\$}▫, were introduced by [Carlet, C.: Two new classes of bent functions. In: Helleseth, T. (ed.) EUROCRYPT 1993. LNCS, vol. 765, pp. 77-101. Springer, Heidelberg (1994)] two decades ago. However, apart from the subclass ▫{\$}{\backslash}mathcal{\{}D{\}}{\_}0{\$}▫, some explicit construction methods for these functions were not provided in [Carlet, C.: Two new classes of bent functions]. Assuming the possibility of specifying a bent function ▫{\$}f{\$}▫ that belongs to one of these two classes (apart from ▫{\$}{\backslash}mathcal{\{}D{\}}{\_}0{\$}▫), the most important issue is then to determine whether ▫{\$}f{\$}▫ is still contained in the known primary classes or lies outside their completed versions. In this article we partially solve this question by providing sufficient conditions on the permutation and related characteristic function (used to define ▫{\$}f{\$}▫ in these classes) so that ▫{\$}f{\$}▫ is provably outside the completed ▫{\$}{\backslash}mathcal{\{}M{\}}{\$}▫ class. To give some existence results, we employ recent results in [Mandal, B., Stanica, P., Gangopadhyay, S., Pasalic, E.: An analysis of ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ class of bent functions. Fundamenta Informaticae 147(3), 271-292 (2016)] where some instances of bent functions in ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ were identified by providing specific permutations and related characteristic functions. More precisely, using our sufficient conditions that apply to both ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ and ▫{\$}{\backslash}mathcal{\{}D{\}}{\$}▫, it is shown that these identified classes of ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ functions described in [Mandal, B., Stanica, P., Gangopadhyay, S., Pasalic, E.: An analysis of ▫{\$}{\backslash}mathcal{\{}C{\}}{\$}▫ class of bent functions] do not belong to the completed ▫{\$}{\backslash}mathcal{\{}M{\}}{\$}▫ class, whereas the question (which is more difficult) whether these functions are also outside the completed partial spread class remains open. We also propose some generic methods for specifying bent functions in {\$}{\backslash}mathcal{\{}D{\}}{\$} outside the completed Maiorana-McFarland class.},
note={Soavtorji: , E. Pasalic, N. Cepak, Y. Wei},
note={Bibliografija: str. 308-309},
note={Abstract},
note={512},
issn={0302-9743},
doi={10.1007/978-3-319-55589-8_20},
url={https://doi.org/10.1007/978-3-319-55589-8_20}
}

