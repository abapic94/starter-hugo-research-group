@Misc{15119705,
author={Pasalic, Enes},
title={Almost fully optimized infinite classes of Boolean functions resistant to (fast) algebraic cryptanalysis},
year={2009},
pages={399-414},
keywords={kriptoanaliza; kriptografija; algebrai{\v{c}}ne lastnosti; anihilatorji; Boolova funkcija; algebraic cryptoanalysis; fast algebraic attacks; algebraic immunity; annihilators; algebraic attack resistant; high degree product; stream ciphers; Boolean function},
abstract={In this paper the possibilities of an iterative concatenation method towards construction of Boolean functions resistant to algebraic cryptanalysis are investigated. The notion of ▫{\$}{\backslash}mathcal{\{}AAR{\}}{\$}▫ (Algebraic Attack Resistant) function is introduced as a unified measure of protection against classical algebraic attacks as well as fast algebraic attacks. Then, it is shown that functions that posses the highest resistance to fast algebraic attacks are necessarily of maximum ▫{\$}{\backslash}mathcal{\{}AI{\}}{\$}▫ (Algebraic Immunity), the notion defined as a minimum degree of functions that annihilate either ▫{\$}f{\$}▫ or ▫{\$}1+f{\$}▫. More precisely, if for any non-annihilating function ▫{\$}g{\$}▫ of degree ▫{\$}e{\$}▫ an optimum degreerelation ▫{\$}e+d {\backslash}ge n{\$}▫ is satisfied in the product ▫{\$}fg=h{\$}▫ (denoting ▫{\$}deg(h)=d{\$}▫), then the function ▫{\$}f{\$}▫ in ▫{\$}n{\$}▫ variables must have maximum ▫{\$}{\backslash}mathcal{\{}AI{\}}{\$}▫, i.e. for nonzero function ▫{\$}g{\$}▫ the relation ▫{\$}fg=0{\$}▫ or ▫{\$}(1+f)g=0{\$}▫ implies. The presented theoretical framework allows us to iteratively construct functions with maximum ▫{\$}{\backslash}mathcal{\{}AI{\}}{\$}▫ satisfying ▫{\$}e+d=n-1{\$}▫, thus almost optimized resistance to fast algebraic cryptanalysis. This infinite class for the first time, apart from almost optimal resistance to algebraic cryptanalysis, in addition generates the functions that possess high nonlinearity (superior to previous constructions) and maximum algebraic degree, thus unifying most of the relevant cryptographic criteria.},
note={Bibliografija: str. 413-414},
note={512.6},
doi={10.1007/978-3-642-00730-9_25},
url={https://doi.org/10.1007/978-3-642-00730-9_25}
}

@Misc{15119193,
author={Pasalic, Enes},
title={On cryptographically significant mappings over GF(2 [sup] n)},
year={2008},
pages={189-204},
keywords={kriptoanaliza; kriptografija; algebrai{\v{c}}ne lastnosti; cryptoanalysis; cryptography; permutation polynomials; power mappings; APN functions; S-box; CCZ-equivalence; algebraic properties},
abstract={In this paper we investigate the algebraic properties of important cryptographic primitives called substitution boxes (S-boxes). An S-box is a mapping that takes ▫{\$}n{\$}▫ binary inputs whose image is a binary ▫{\$}m{\$}▫-tuple; therefore it is represented as ▫{\$}F:{\backslash}text{\{}GF{\}}(2)^n {\backslash}rightarrow {\backslash}text{\{}GF{\}}(2)^m{\$}▫. One of the most important cryptographic applications is the case ▫{\$}n = m{\$}▫, thus the S-box may be viewed as a function over ▫{\$}{\backslash}text{\{}GF{\}}(2^n){\$}▫. We show that certain classes of functions over ▫{\$}{\backslash}text{\{}GF{\}}(2^n){\$}▫ do not possess a cryptographic property known as APN (AlmostPerfect Nonlinear) permutations. On the other hand, when ▫{\$}n{\$}▫ is odd, an infinite class of APN permutations may be derived in a recursive manner, that is starting with a specific APN permutation on ▫{\$}{\backslash}text{\{}GF{\}}(2^k), k{\$}▫ odd, APN permutations are derived over ▫{\$}{\backslash}text{\{}GF{\}}(2^{\{}k+2i{\}}){\$}▫ for any ▫{\$}i {\backslash}geq 1{\$}▫. Some theoretical results related to permutation polynomials and algebraic properties of the functions in the ring ▫{\$}{\backslash}text{\{}GF{\}}(q)[x,y]{\$}▫ are also presented. For sparse polynomials over the field ▫{\$}{\backslash}text{\{}GF{\}}(2^n){\$}▫, an efficient algorithm for finding low degree I/O equations is proposed.},
note={Bibliografija: str. 202-203},
note={512.6},
doi={10.1007/978-3-540-69499-1_16},
url={https://doi.org/10.1007/978-3-540-69499-1_16}
}

