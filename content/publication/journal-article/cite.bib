@Article{18709849,
author={Cepak, Nastja
and Pasalic, Enes
and Muratovi{\'{c}}-Ribi{\'{c}}, Amela},
title={Frobenius linear translators giving rise to new infinite classes of permutations and bent functions},
year={2019Nov.},
volume={11},
number={6},
pages={1275-1295},
keywords={Frobeniusov translator; linearni translator; permutacije; ukrivljene funkcije; translators; finite fields; permutations; bent functions},
abstract={We show the existence of many infinite classes of permutations over finite fields and bent functions by extending the notion of linear translators, introduced by Kyureghyan (J. Combin. Theory Ser. A 118(3), 1052-1061, 2011). We call these translators Frobenius translators since the derivatives of ▫{\$}f : F{\_}{\{}p^n{\}} {\backslash}rightarrow F{\_}{\{}p^k{\}}{\$}▫, where ▫{\$}n = rk{\$}▫, are of the form ▫{\$}f(x + u{\backslash}phi) - f(x) = u^{\{}p^i{\}}b{\$}▫, for a fixed ▫{\$}b {\backslash}in F{\_}{\{}p^k{\}}{\$}▫ and all ▫{\$}u {\backslash}in F{\_}{\{}p^k{\}}{\$}▫, rather than considering the standard case corresponding to ▫{\$}i = 0{\$}▫. This considerably extends a rather rare family ▫{\$}{\backslash}{\{}f{\backslash}{\}}{\$}▫ admitting linear translators of the above form. Furthermore, we solve a few open problems in the recent article (Cepak et al., Finite Fields Appl. 45, 19-42, 2017) concerning the existence and an exact specification of ▫{\$}f{\$}▫ admitting classical linear translators, and an open problem introduced in Hod{\v{z}}i{\'{c}} et al. (2018), of finding a triple of bent functions ▫{\$}f{\_}1, f{\_}2, f{\_}3{\$}▫ such that their sum ▫{\$}f{\_}4{\$}▫ is bent and that the sum of their duals ▫{\$}f{\_}1^{\backslash}ast +f{\_}2^{\backslash}ast +f{\_}3^{\backslash}ast +f{\_}4^{\backslash}ast = 1{\$}▫. Finally, we also specify two huge families of permutations over ▫{\$}F{\_}{\{}p^n{\}}{\$}▫ related to the condition that ▫{\$}G(y) = -L(y)+(y+{\backslash}delta)^s -(y+{\backslash}delta)^{\{}p^ks{\}}{\$}▫ permutes the set ▫{\$}S ={\backslash}{\{}{\backslash}beta {\backslash}in F{\_}{\{}p^n{\}} : Tr^n{\_}k({\backslash}beta) = 0{\backslash}{\}}{\$}▫, where ▫{\$}n = 2k{\$}▫ and ▫{\$}p {\&}gt; 2{\$}▫. Finally, we offer generalizations of constructions of bent functions in Mesnager et al. (2017) and described some new bent families using the permutations found in Cepak et al. (Finite Fields Appl. 45, 19-42, 2017).},
note={Spletna objava (online first): 24. 8. 2019},
note={Bibliografija: str. 1294},
note={Abstract},
note={512.6},
issn={1936-2447},
doi={10.1007/s12095-019-00395-1},
url={doi:10.1007/s12095-019-00395-1},
url={https://doi.org/10.1007/s12095-019-00395-1}
}

@Article{1540928196,
